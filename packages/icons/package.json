{
  "name": "@onwo/icons",
  "version": "0.525.0",
  "url": "https://ui.onwo.cloud/",
  "description": "An icon library ported to qwik.js from the moon.io DS",
  "repository": "https://github.com/onwo-cloud/onwo-ui",
  "license": "MIT",
  "main": "./lib/index.qwik.mjs",
  "qwik": "./lib/index.qwik.mjs",
  "types": "./lib-types/index.d.ts",
  "keywords": [
    "tailwindcss",
    "icon-library",
    "qwik.js"
  ],
  "files": [
    "lib",
    "lib-types"
  ],
  "engines": {
    "node": "^22"
  },
  "private": false,
  "type": "module",
  "scripts": {
    "generate-icons": "bun generate-icons.ts && yarn lint.fix",
    "build": "ESLINT_MODE=full qwik build",
    "build.types": "tsc --emitDeclarationOnly --incremental --pretty",
    "build.lib": "vite build --mode lib",
    "dev": "vite build --mode lib -c ./vite.config.dev.ts",
    "lint": "eslint \"src/**/*.ts*\"",
    "lint.fix": "npm run lint -- --fix",
    "ts-watch": "tsc --emitDeclarationOnly --incremental --preserveWatchOutput --pretty --watch"
  },
  "devDependencies": {
    "@onwo/primitives": "link:../primitives",
    "@builder.io/qwik": "1.14",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "24.0.3",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "peerDependencies": {
    "@onwo/primitives": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
