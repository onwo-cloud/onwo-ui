{
  "name": "@onwo/ui",
  "version": "0.0.12-alpha",
  "url": "https://ui.onwo.cloud/",
  "description": "An ui library for qwik.js inspired by moon.io",
  "repository": "https://github.com/onwo-cloud/onwo-ui",
  "license": "MIT",
  "main": "./lib/index.qwik.mjs",
  "qwik": "./lib/index.qwik.mjs",
  "types": "./lib-types/index.d.ts",
  "keywords": [
    "tailwindcss",
    "ui-library",
    "qwik.js"
  ],
  "files": [
    "lib",
    "lib-types"
  ],
  "engines": {
    "node": "^22"
  },
  "private": false,
  "type": "module",
  "scripts": {
    "build": "ESLINT_MODE=full qwik build",
    "build.types": "tsc --emitDeclarationOnly --incremental --pretty",
    "build.lib": "vite build --mode lib",
    "dev": "vite build --mode lib -c ./vite.config.dev.ts",
    "dev.build": "ESLINT=false qwik build",
    "lint": "eslint \"src/**/*.ts*\"",
    "lint.fix": "npm run lint -- --fix",
    "ts-watch": "tsc --emitDeclarationOnly --incremental --preserveWatchOutput --pretty --watch"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.14",
    "@onwo/icons": "link:../icons",
    "@onwo/tailwindcss": "link:../tailwindcss",
    "@onwo/primitives": "link:../primitives",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "24.0.3",
    "@types/tailwindcss": "^3.1.0",
    "chokidar": "^4.0.3",
    "np": "^10.2.0",
    "tailwindcss": "^4.1.10",
    "typescript": "5.8.3",
    "undici": "*",
    "vite": "6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "date-fns": "^4.1.0"
  },
  "peerDependencies": {
    "@onwo/icons": "*",
    "@onwo/primitives": "*",
    "@onwo/tailwindcss": "*",
    "date-fns": "^2.0.0 || ^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.10"
  }
}
